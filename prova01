#Quest達o1

defmodule calculadora do
   def calculaFatorial(0), do: 1
   def calculaFatorial(num) when num>0 do
     Enum.reduce(1..num, &*/2)
   end

   def calculaPoncia(num,potencia) do
     :matyh.pow(num,potencia)
     |> round()
   end
  
  def informaPercentual(num, percentual) do
  |
  end
end

